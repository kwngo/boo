<!-- Everything inside the #post tags pulls data from the static page -->
{{!< default}}

{{> "site-header"}}

{{#post}}

<article class="{{post_class}} ">
  <header class="page-header">
    <h1 class="page-title">{{title}}</h1>
    <section class="page-meta">
    </section>
  </header>
  <section class="page-content">
    {{content}}
  </section>
</article>
{{/post}}
